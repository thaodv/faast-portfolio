  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-faast/faast-portfolio.git"
  },
  "scripts": {
    "start": "ws --https --spa dist/index.html --rewrite '/portfolio/* -> /dist/$1'",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --https",
    "lint": "eslint src/ && tslint -p .",
    "test": "npm run lint",
    "build:webpack": "webpack --progress",
    "build:ipfs": "cross-env IPFS=true npm run build:webpack",
    "build": "npm run lint && npm run build:webpack",
    "preversion": "git checkout develop",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "release": "git checkout master && git merge develop && git push && git checkout develop",
    "postinstall": "rm -f node_modules/web3/index.d.ts"
  },
  "standard": {
    "ignore": [
      "dist/",
      "res/"
    ]
  },
